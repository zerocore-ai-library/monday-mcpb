{
  "name": "@mondaydotcomorg/agent-toolkit",
  "version": "2.36.0",
  "description": "monday.com agent toolkit",
  "exports": {
    "./mcp": {
      "types": "./dist/esm/mcp/index.d.ts",
      "import": "./dist/esm/mcp/index.js",
      "require": "./dist/cjs/mcp/index.js"
    },
    "./core": {
      "types": "./dist/esm/core/index.d.ts",
      "import": "./dist/esm/core/index.js",
      "require": "./dist/cjs/core/index.js"
    },
    "./openai": {
      "types": "./dist/esm/openai/index.d.ts",
      "import": "./dist/esm/openai/index.js",
      "require": "./dist/cjs/openai/index.js"
    }
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "prettier": "prettier --write \"src/**/*.ts\" --ignore-path \"../../.prettierignore\"",
    "lint": "eslint --fix \"src/**/*.ts\" --ignore-path \"../../.eslintignore\"",
    "test": "jest -c",
    "watch": "rollup -c -w",
    "fetch:schema": "bash fetch-schema.sh",
    "codegen": "bash codegen.sh",
    "fetch:generate": "bash fetch-and-generate.sh"
  },
  "engines": {
    "node": ">= 16.20.0"
  },
  "author": "monday.com AI Team",
  "license": "MIT",
  "keywords": [
    "monday",
    "api",
    "agent-toolkit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mondaycom/monday-ai/tree/master/packages/agent-toolkit"
  },
  "dependencies": {
    "@mondaydotcomorg/api": "^13.0.0",
    "axios": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.2",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.18",
    "jest": "^29.7.0",
    "moment": "^2.30.1",
    "openai": "^4.93.0",
    "rollup": "^2.79.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.3",
    "ts-jest": "^29.1.2",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.13.2",
    "openai": "^4.93.0"
  }
}
